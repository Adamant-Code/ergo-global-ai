{
  "name": "@monorepo/server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prebuild": "cp -r ../../node_modules/adminjs node_modules/ && cp -r ../../node_modules/@adminjs node_modules/ && tsx bundleComponents.ts",
    "build": "tsc && tsc-alias && cp -r public dist/ && cp -r .adminjs dist/",
    "postbuild": "mkdir -p dist/public/admin/frontend/assets && cp -r public/*.bundle.js dist/public/admin/frontend/assets/",
    "start": "NODE_ENV=production ADMIN_JS_SKIP_BUNDLE=true DEBUG=server/*,AdminJS*,adminjs:* node dist/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch --exclude \"node_modules\" --exclude \"dist\" --exclude \"./src/routes/admin/client/**/*\" --tsconfig ./tsconfig.json src/index.ts",
    "watch": "tsc --watch",
    "seed": "tsx knex/seed.ts",
    "make:migration": "NODE_OPTIONS=\"--no-warnings --loader=ts-node/esm --experimental-specifier-resolution=node\" knex migrate:make --knexfile ./knex/knexfile.ts",
    "migrate:rollback": "NODE_OPTIONS=\"--no-warnings --loader=ts-node/esm --experimental-specifier-resolution=node\" knex migrate:rollback --knexfile ./knex/knexfile.ts",
    "migrate": "NODE_OPTIONS=\"--no-warnings --loader=ts-node/esm --experimental-specifier-resolution=node\" knex migrate:latest --knexfile ./knex/knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/import-export": "^3.0.0",
    "@adminjs/logger": "^5.0.1",
    "@adminjs/objection": "^2.0.3",
    "@adminjs/passwords": "^4.0.0",
    "@adminjs/prisma": "^5.0.3",
    "@adminjs/themes": "^1.0.1",
    "@prisma/client": "^5.22.0",
    "@request-response/types": "*",
    "adminjs": "^7.8.15",
    "ajv-formats": "^3.0.1",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "objection": "^3.1.5",
    "pg": "^8.16.0",
    "redis": "^4.7.0",
    "redoc-express": "^2.1.0",
    "socket.io": "^4.8.1",
    "stripe": "^18.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@adminjs/bundler": "^3.0.0",
    "@adminjs/cli": "^1.0.5",
    "@adminjs/design-system": "^4.1.1",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/node-fetch": "^2.6.12",
    "@types/redis": "^4.0.10",
    "@types/stripe": "^8.0.417",
    "@types/styled-components": "^5.1.34",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
